{
	"name": "verbose-logging",
	"description": "Tools for logging",
	"version": "0.0.1",
	"publisher": "vscode",
	"enableProposedApi": true,
	"private": true,
	"engines": {
		"vscode": "*"
	},
	"contributes": {
		"commands": [
			{
				"command": "verbose-logging.previewOrUpload",
				"title": "Preview or Upload Diagnostic Artifacts"
			}
		]
	},
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "7.0.43",
		"vscode": "1.1.5"
	},
	"dependencies": {
		"archiver": "^2.1.0",
		"azure-storage": "^2.6.0"
	}
}
